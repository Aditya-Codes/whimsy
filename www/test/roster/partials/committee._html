_h1 do
  _a '{{pmc.cn}}', href: 'http://{{pmc.cn}}.apache.org/'
end

_h2 'PMC'

_table do
  _tr ng_repeat: 'member in pmc_members | orderBy:"uid"' do
    _td do
      _a href: 'committer/{{ member.uid }}' do
        _span ng_bind: 'member.uid'
      end
    end
    _td ng_bind: 'member.cn', asf_id: '{{ member.uid }}'
  end
end

_h2 'Committers'

_table do
  _tr ng_repeat: 'committer in pmc_committers | orderBy:"uid"' do
    _td do
      _a href: 'committer/{{ committer.uid }}' do
        _span ng_bind: 'committer.uid'
      end
    end
    _td ng_bind: 'committer.cn', asf_id: '{{ committer.uid }}'
  end
end
